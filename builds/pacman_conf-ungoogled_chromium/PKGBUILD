pkgname=pacman_conf-ungoogled_chromium
pkgver=1
pkgrel=1
pkgdesc="Install pacman configuration (pacman.conf) for ungoogled chromium"
arch=(any)
license=(GPL)
source=(ungoogled-chromium.conf)
install=pacman_conf.install

package() {
  install -Dm0644 ungoogled-chromium.conf \
    "$pkgdir"/etc/pacman.d/ungoogled-chromium.conf
}
sha256sums=('7fd0f0066eb367c999b837f6c2731f9e32831dbe117b3bdf76989d3443701016')
