# Maintainer: Carles Rafols i Belles

pkgname=pacman_conf-system_config
pkgver=1
pkgrel=1
pkgdesc="Install pacman configuration (pacman.conf) for system_config"
arch=(any)
license=(GPL)
source=(system_config.conf)
install=pacman_conf.install

package() {
  install -Dm0644 system_config.conf \
    "$pkgdir"/etc/pacman.d/system_config.conf
}
sha256sums=('5aab93165c814e57fa30b8a061b5d5c6cb8462953ec53591edf4611442835e42')
