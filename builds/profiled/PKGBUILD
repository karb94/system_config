# Maintainer: Carles Rafols i Belles

pkgname=profiled
pkgver=1
pkgrel=1
pkgdesc="Set system-wide environmental variables in /etc/profile.d/"
arch=(any)
license=(GPL)
source=(system_config.sh)

package() {
  install -Dm0644 system_config.sh "$pkgdir"/etc/profile.d/system_config.sh
}
sha256sums=('3a2ef57cb693ab52d087fed58fcd325c6deb9abb8fb9cf1a8f071a140b580e99')
